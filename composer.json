{
  "name": "delboy1978uk/common",
  "description": "Stuff that winds up in every project",
  "license": "MIT",
  "authors": [
    {
      "name": "Derek Stephen McLean",
      "email": "delboy1978uk@gmail.com"
    }
  ],
  "require": {
    "delboy1978uk/barnacle": "^2.1",
    "doctrine/orm": "^2.6.3",
    "doctrine/migrations": "1.8.1",
    "symfony/console": ">=2.7 <5.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "codeception/codeception": "^2.5.6"
  },
  "autoload": {
    "psr-4": {
      "Del\\Common\\": "src/",
      "DelTesting\\": "tests/unit/Del/"
    }
  },
  "bin": [
    "bin/migrant"
  ],
  "scripts": {
    "post-install-cmd": [
      "[[ -d migrations ]] || mkdir migrations"
    ],
    "post-update-cmd": [
      "[[ -d migrations ]] || mkdir migrations"
    ]
  }
}
