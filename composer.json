{
    "name": "delboy1978uk/common",
    "description": "Stuff that winds up in every project",
    "license": "MIT",
    "authors": [
        {
            "name": "Derek Stephen McLean",
            "email": "delboy1978uk@gmail.com"
        }
    ],
    "require": {
      "delboy1978uk/barnacle": "^2.2",
      "doctrine/orm": "^2.6.3",
      "doctrine/migrations": "2.2",
      "symfony/console": ">=4.0"
    },
    "require-dev":{
        "roave/security-advisories": "dev-master",
        "codeception/codeception":"^4.0"
    },
    "autoload":{
        "psr-4":{
            "Del\\Common\\":"src/",
            "DelTesting\\": "tests/unit/Del/"
        }
    },
    "bin": [ "bin/migrant"],
    "scripts": {
        "post-install-cmd": [
            "[[ -d data ]] || mkdir data",
            "[[ -d data/migrations ]] || mkdir data/migrations"
        ],
        "post-update-cmd": [
            "[[ -d data ]] || mkdir data",
            "[[ -d data/migrations ]] || mkdir data/migrations"
        ]
    }
}
